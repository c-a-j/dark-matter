---
import Layout from "../layouts/Layout.astro";
import SEO from "../components/common/SEO.astro";
import Section from "../components/common/Section.astro";
---

<Layout>
  <Fragment slot="head">
    <SEO configKey="notFound" />
  </Fragment>
  <Section class="my-16">
    <h1
      class="font-display text-3xl text-theme-dark sm:text-4xl dark:text-theme-light"
    >
      404
    </h1>
    <p class="text-theme-dark-muted dark:text-theme-light-muted">
      The page you are looking for does not exist.
    </p>
  </Section>
</Layout>
